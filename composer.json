{
    "name": "douglasgreen/dateparser",
    "description": "A PHP library to parse dates, times, and recurring date expressions",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/douglasgreen/dateparser",
    "authors": [
        {
            "name": "Douglas Green",
            "email": "douglas@nurd.site",
            "homepage": "https://nurd.site/",
            "role": "Developer"
        }
    ],
    "keywords": [
        "date",
        "time",
        "date parser",
        "time parser",
        "date interpretation",
        "natural language date",
        "PHP",
        "recurring dates",
        "date expressions",
        "time expressions"
    ],
    "autoload": {
        "psr-4": {
            "DouglasGreen\\DateParser\\": "src/",
            "DouglasGreen\\DateParser\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/douglasgreen/config-setup"
        }
    ],
    "require": {
        "php": ">=8.1",
        "douglasgreen/utility": "dev-main"
    },
    "require-dev": {
        "douglasgreen/config-setup": "dev-main",
        "phpmd/phpmd": "^2.15",
        "phpstan/phpstan": "^1.11",
        "phpunit/phpunit": "^10.5",
        "rector/rector": "^1.1",
        "symplify/easy-coding-standard": "^12.3"
    },
    "scripts": {
        "lint": [
            "ecs",
            "php-linter",
            "phpstan analyse",
            "phpmd --cache --cache-file var/cache/phpmd/cache.php . text phpmd.xml",
            "rector --dry-run"
        ],
        "lint:fix": [
            "ecs --fix",
            "rector"
        ],
        "post-install-cmd": "config-setup",
        "post-update-cmd": "config-setup",
        "test": "phpunit"
    }
}
