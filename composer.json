{
    "name": "douglasgreen/dateparser",
    "description": "A PHP library to parse dates, times, and recurring date expressions",
    "type": "library",
    "keywords": [
        "date",
        "time",
        "date parser",
        "time parser",
        "date interpretation",
        "natural language date",
        "PHP",
        "recurring dates",
        "date expressions",
        "time expressions"
    ],
    "homepage": "https://github.com/douglasgreen/dateparser",
    "license": "MIT",
    "authors": [
        {
            "name": "Douglas Green",
            "email": "douglas@nurd.site",
            "homepage": "https://nurd.site/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.93.1",
        "phpstan/phpstan": "^2.1.38",
        "phpunit/phpunit": "^11.5",
        "rector/rector": "^2.3.5"

    },
    "autoload": {
        "psr-4": {
            "DouglasGreen\\DateParser\\": "src/",
            "DouglasGreen\\DateParser\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "lint": [
            "php-cs-fixer fix --dry-run --verbose",
            "phpstan analyse",
            "rector process --dry-run"
        ],
        "lint:fix": [
            "php-cs-fixer fix",
            "rector process"
        ],
        "test": "phpunit"
    }
}
