{
    "name": "douglasgreen/dateparser",
    "description": "A PHP library to parse dates, times, and recurring date expressions",
    "type": "library",
    "keywords": [
        "date",
        "time",
        "date parser",
        "time parser",
        "date interpretation",
        "natural language date",
        "PHP",
        "recurring dates",
        "date expressions",
        "time expressions"
    ],
    "homepage": "https://github.com/douglasgreen/dateparser",
    "license": "MIT",
    "authors": [
        {
            "name": "Douglas Green",
            "email": "douglas@nurd.site",
            "homepage": "https://nurd.site/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "douglasgreen/utility": "^0.5"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.5",
        "rector/rector": "^2.0",
        "shipmonk/name-collision-detector": "^2.1",
        "symplify/easy-coding-standard": "^12.3"
    },
    "autoload": {
        "psr-4": {
            "DouglasGreen\\DateParser\\": "src/",
            "DouglasGreen\\DateParser\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "lint": [
            "detect-collisions",
            "ecs",
            "phpstan analyse",
            "rector --dry-run"
        ],
        "lint:fix": [
            "ecs --fix",
            "rector"
        ],
        "test": "phpunit"
    }
}
